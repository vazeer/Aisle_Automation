<script type="text/javascript" src="http://mcdn.zulilyinc.com/js/cache/af51db65a3e4d38f94d2968f39924184.js"></script>
<script type="text/javascript">

function isProductUrl(href) {
	return(
		(href.match('\.zulily\.com\/p\/') || href.match('^\/p\/')) ||
		(href.match('\.zulily\.com\/search') || href.match('^\/search')) ||
		(href.match('\.zulily\.com\/shutterfly') || href.match('^\/shutterfly')) ||
		($j('#search-view').length)
	);
}

function isEventUrl(href) {
	return(
		(href.match('\.zulily\.com\/e\/') || href.match('^\/e\/'))
	);
}

function isHomepage(href) {
	if ($j('#shop-by-age-events').length || $j('#home-page-content').length) {
		return true;
	}
	return false;
}

// links that always require login
function isProtected($o) {
	eid = $o.attr('id');
	if (eid == 'love-this-brand-link' || eid == 'notify-me' || eid == 'invite-friends-link') {
		return true;
	}
	else if ($o.hasClass('icon-share')) {
		// share icons
		return true;
	}
	return false;
}

// what can you view?
var _canViewProducts = false;
var _canViewEvents = false;
var _canViewHomepage = false;
var _cmsPreviewModal = false;
var _canViewPage = false;

$j(document).ready(function() {
	$j("#signup_signin_modal a.signin-link").click(function() {
		$j('#signup_modal').hide();
		$j('#login_modal').show();
		$j.ajax( {
			url: "/track/event.gif?category=modal&type=login",
			cache: false,
			success: function(){}
		});
	});

	$j("#signup-link").click(function() {
		$j('#login_modal').hide();
		$j('#signup_modal').show();
		$j.ajax( {
			url: "/track/event.gif?category=modal&type=signup",
			cache: false,
			success: function(){}
		});
	});

	$j("#input-email-to-password").click(function() {
		if (validateForm('#email_address')) {
			$j('#enter-email').hide();
			$j('#enter-password').show();
			$j('#password').focus();
			$j('#welcome').html('Welcome, ' + $j('#email_address').val());
		} else {
			$j('#email_address').focus();
		}
	});

	$j("#backToEmail").click(function() {
		$j('#enter-password').hide();
		$j('#enter-email').show();
		$j('#email_address').focus();
		$j('#password').val();
	});

	var submitted = 0;
	$j("#start-shopping").click(function() { //submit after 2step process
		if (submitted==1) {
			return false;
		}
		else {
			validateForm('#email_address');
			if (!validateForm('#password')) {
				$j('#password').focus();
			}
			else {
				$j('#password-advice').html('').css('display','none');
				$j('#password').removeClass('validation-failed');
				$j('#start-shopping').removeClass('red').removeAttr('href');
				$j('#signup_form').submit();
				submitted=1;
			}
		}
	});

	$j("#continue-to-name").click(function() {
		if (!validateForm('#password')) {
			$j('#password').focus();
		}
		else {
			$j('#password-advice').html('').css('display','none');
			$j('#password').removeClass('validation-failed');
			$j('#start-shopping').removeClass('red').removeAttr('href');
			$j('#enter-password').hide();
			$j('#enter-name').show();
			$j('#firstname').focus();
		}
	});

	$j("#continue-to-password").click(function() {
		$j('#enter-name').hide();
		$j('#enter-password').show();
		$j('#password').focus();
	});

	$j("#start-shopping-novalidate").click(function() { //submit after step 3: name
		if (submitted==1) {
			return false;
		}
		else {
			$j('#signup_form').submit();
			submitted=1;
		}
	});

	$j("#sign-in-now").click(function() {
		if (submitted==1) {
			return false;
		}
		else {
			if (!validateForm('#login_email_address')) {
				$j('#login_email_address').focus();
			}
			else if (!validateForm('#login_password')) {
				$j('#login_password').focus();
			}
			else {
				$j('#login_password-advice').html('').css('display','none');
				$j('#login_password').removeClass('validation-failed');
				$j('#sign-in-now').removeClass('red').removeAttr('href');
				$j('#login_form').submit();
				submitted=1;
			}
		}
	});

	// Just for the ipad modal test
	$j('#ipad-modal-form').on('submit', function(e) {
		e.preventDefault();

		if (submitted==1) {
			return false;
		}
		else {
			if (!validateForm('#ipad-email')) {
				$j('#ipad-email').focus();
				$j.fn.colorbox.resize();
			}
			else if (!validateForm('#ipad-password')) {
				$j('#ipad-password').focus();
				$j.fn.colorbox.resize();
			}
			else {
				$j('.advice', this).hide();
				$j('button[type=submit]',this).removeClass('red');

				submitted = 1;

				$j(this).off('submit');
				this.submit();
			}
		}
	});

	function validateForm(input) {
		// hide the facebook button if they have tried to signup with email
		$j('div.modal_facebook_signup, div.modal_facebook_signin').hide();

		var $element = $j(input);
		var $value = $element.val();
		$value = $j.trim($value);

		// validate length is more than 6 chars
		if ($value.length == 0) {
			$element.addClass("validation-failed");
			$j('#' + $element.attr('id') + '-advice').html('This is a required field.').fadeIn();
			return false;
		} else if ($value.length > 0) {
		 	if ($value.length < 6)  {
				$element.addClass("validation-failed");
				$j('#' + $element.attr('id') + '-advice').html('Please enter 6 or more characters.').fadeIn();
				return false;
			}
			if ($value.length >= 6) {
				// validate that it looks like an email if has class "email"
				if ($element.hasClass("email")) {
					// this pattern is borrowed from http://stackoverflow.com/a/3613106
					var pattern = /^\s*[\w\-\+_]+(?:\.[\w\-\+_]+)*\@[\w\-\+_]+\.[\w\-\+_]+(?:\.[\w\-\+_]+)*\s*$/;

					if (!pattern.test($value)) {
						$element.addClass("validation-failed");
						$j('#'+$element.attr('id')+'-advice').html('Please enter a valid email address').fadeIn();
						return false;
					} else {
						$element.removeClass("validation-failed");
						$j('#' + $element.attr('id') + '-advice').html('').fadeOut();
					}
				}

				if (input == '#email_address') {
					var valid = true;
					function showSuggestedDomain(data) {
						suggested_email = emailAddress[0]+ '@'+ data;
						$j('#email-label').hide();
						$j('#email_address').toggle(); //hide caused css artifacts
						$j('#email-suggestion').show();
						$j("#input-email-to-password").hide();
						$j('#emailNotice').html('Did you mean '+ suggested_email+ '?');
						$j('#wrongEmail').html($j('#email_address').val());
						$j('#suggestedEmail').val(emailAddress[0]+ '@'+ data);
						return false;
					}

					// validate with emailDomainTool
					var emailAddress = $value.split('@');
					domain = emailAddress[1];
					$j.ajax({
						url : '/checkEmailDomain',
						type : 'POST',
						data : {
							wrong_domain : domain ? domain : ''
						},
					 	async : false,
						success : function(data) {
							if (data) {
								// if there is data, it means a suggested domain was returned
								valid = false;
								showSuggestedDomain(data);
							}
						}
					});

					return valid;
				}

			}
		}
		return true;
	}
	// buttons for email domain suggestions
	$j('#yesButton').click(function() {
		$j('#email_address').val($j('#suggestedEmail').val());
		$j('#enter-email').hide();
		$j('#enter-password').show();
		$j('#password').focus();
		$j('#welcome').html('Welcome, ' + $j('#email_address').val());
	});

	$j('#noButton').click(function() {
		$j('#enter-email').hide();
		$j('#enter-password').show();
		$j('#password').focus();
		$j('#welcome').html('Welcome, ' + $j('#email_address').val());
	});


	//capture the return key
	$j("#email_address").bind("keydown", function(e) {
		if (e.keyCode == 13) {
			// these setTimeouts are hacks to deal with Firefox firing events in the wrong order when using autocomplete
			// see: http://stackoverflow.com/questions/2303955/i-have-a-problem-with-keydown-event-and-autocomplete-in-firebox-on-mac
			setTimeout(function() {
				$j("#input-email-to-password").click();
			}, 0);
            return false;
		}
	});
	$j("#password").bind("keydown", function(e) {
		if (e.keyCode == 13) {
			$j("#continue-to-name").click();
			return false;
		}
	});
    $j("#lastname").bind("keydown", function(e) {
		if (e.keyCode == 13) {
			$j("#start-shopping-novalidate").click();
			return false;
		}
	});
	$j("#login_password").bind("keydown", function(e) {
		if (e.keyCode == 13) {
			$j("#sign-in-now").click();
			return false;
		}
	});

	$j("#radioBlock a").click(function(e){
		e.preventDefault();
		$j("#radioBlock .radioInput").toggle();
		$j("div.modal_facebook_signup").hide();
		$j("#radioCodeClick").val('yes');
		$j("#radioCode").focus();
		return false;
	});

    	// these handle the form interactions and trigger the signup over the (product, event, etc) page
	$j('#productAddToCartForm, #search-form').submit(function(e) {
		e.preventDefault();

		// get the form data they were trying to post
		var form = {};
		form['form'] = $j(this).attr('id');
		form['action'] = $j(this).attr('action');
		form['params'] = $j(this).serializeObject();

		// add it as a hidden field
		$j('<input type="hidden" name="ffoorrmm" value="'+encodeURIComponent(JSON.stringify(form))+'" />').appendTo('#signup_form, #login_form');

		// fire the signup modal
		signupCB("#signup_signin_modal", "414px", "450px");
	});

	// we're just going to redirect to the real refer a friend page if they try it before being logged in
	$j('#referafriend-form').submit(function(e) {
		e.preventDefault();

		// add the url they were trying to get to as a hidden field
		$j('<input type="hidden" name="redirectUrl" value="'+encodeURIComponent('/invite/refer')+'" />').appendTo('#signup_form, #login_form');

		// fire the signup modal
		signupCB("#signup_signin_modal", "414px", "450px");
	});

									$j("body").delegate("a", "click", function(e) {
			var $this = $j(this);
			var href = $this.attr('href');
			if (href !== undefined) {
				// check globally protected links, product links, and event links
				if (isProtected($this) || (!_canViewProducts && isProductUrl(href)) || (!_canViewEvents && isEventUrl(href))) {
					e.preventDefault();
					// add the url they were trying to get to as a hidden field
					$j('<input type="hidden" name="redirectUrl" value="'+encodeURIComponent(href)+'" />').appendTo('#signup_form, #login_form');
					signupCB("#signup_signin_modal", "414px", "450px");
				}
			}
		});
		$j('<input type="hidden" name="redirectUrl" value="'+encodeURIComponent(document.location.href)+'" />').appendTo('#signup_form, #login_form');

						href = document.location.href;

		if (_cmsPreviewModal || (!_canViewPage && ((!_canViewHomepage && isHomepage(href)) || (!_canViewEvents && isEventUrl(href)) || (!_canViewProducts && isProductUrl(href))))) {
			setTimeout('signupCB("#signup_signin_modal", "' + '414px' + '", "' + '450px' + '")', 500);
		}
	});

signupCB = function(el, w, h) {
	
    var closeable = false;

	$j.fn.colorbox({
		transition: "none",
		inline:true,
		open:true,
		opacity:"0.5",
		overlayClose:closeable,
		escKey:closeable,
		scrolling:false,
		innerWidth: w,
		innerHeight: h,
		href: el,
		fixed: true	});

	setTimeout("$j('#email_address').focus()",1000)	;
	$j(document).bind('cbox_cleanup', function(element){
	    $j(el).hide();
	});

	// signup modal visitor tracking
	trackAction('signup-view', 'modal_signup');

	$j.ajax( {
		url: "/track/event.gif?category=modal&type=signup",
		cache: false,
		success: function(){}
	});
};

   var welcomeModalImage = new Image();
   welcomeModalImage.src = "https://a248.e.akamai.net/media.zulily.com/images/newmodalv2.gif";

</script>

